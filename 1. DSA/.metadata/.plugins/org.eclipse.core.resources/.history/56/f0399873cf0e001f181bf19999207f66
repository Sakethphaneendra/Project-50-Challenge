package ArraysPlayLIst;

import java.util.*;
public class Largest_and_Smallest {

	public static void main(String[] args) {
//		Implement a Java program to find the largest and smallest elements in an array.
		int Biggest = 1,Smallest = 0;
		Scanner in = new Scanner(System.in);
		int Array[] = new int[10];
		for(int i=0;i<10;i++) {
			Array[i]=in.nextInt();
			if(Array[i]>=Smallest) {
				Biggest = Array[i];
			}
			
		}
		in.close();
	}

}
