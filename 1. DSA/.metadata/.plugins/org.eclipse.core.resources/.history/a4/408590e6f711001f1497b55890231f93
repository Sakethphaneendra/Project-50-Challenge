//		1.Push
//		2.Pop
//		3.Peek
//		4.Search  General Operations 
		
		Stack<String> Colors = new Stack<>();
		
		Colors.push("Red");
		Colors.push("Peel");
		Colors.push("Black");
		System.out.println(Colors);
		
		System.out.println(Colors.peek());
		System.out.println(Colors);
		
		System.out.println(Colors.search("Red"));
		System.out.println(Colors);

	}

}
