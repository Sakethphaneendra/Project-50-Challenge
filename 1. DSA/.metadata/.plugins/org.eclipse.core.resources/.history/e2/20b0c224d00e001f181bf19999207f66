package ArraysPlayLIst;

import java.util.*;
public class Largest_and_Smallest {

	public static void main(String[] args) {
//		Implement a Java program to find the largest and smallest elements in an array.
		int Biggest = 0,Smallest = 0;
		Scanner in = new Scanner(System.in);
		int Array[] = new int[5];
		for(int i=0;i<Array.length;i++) {
			Array[i]=in.nextInt();
			if(Array[i]>=Biggest) {
				Biggest = Array[i];
			}
			if(Array[i]<=Smallest) {
				Smallest = Array[i];
			}	
		}
		System.out.println("Largest Element : "+Biggest +"\nSmallest Elements : "+Smallest);
		in.close();
	}

}
